{
  "$schema": "https://ui.shadcn.com/schema/registry-item.json",
  "name": "warp-dialog-demo",
  "type": "registry:example",
  "title": "Warp dialog demo",
  "author": "Rushil Dhinoja",
  "description": "A demonstration of the warp dialog component showcasing its 3D perspective animations with a welcome message and action buttons.",
  "dependencies": [],
  "registryDependencies": [
    "https://moleculeui.design/r/warp-dialog",
    "\"button\""
  ],
  "files": [
    {
      "path": "registry/example/warp-dialog-demo.tsx",
      "content": "import { Button } from \"@/components/ui/button\"\nimport {\n  WarpDialog,\n  WarpDialogContent,\n  WarpDialogTrigger,\n} from \"@/registry/molecule-ui/warp-dialog\"\n\nexport function WarpOverlayDemo() {\n  return (\n    <div className=\"relative flex h-full w-full items-center justify-center overflow-hidden rounded-2xl\">\n      <WarpDialog>\n        <WarpDialogTrigger asChild>\n          <Button variant=\"outline\">Open Dialog</Button>\n        </WarpDialogTrigger>\n        <WarpDialogContent>\n          <h2 className=\"mb-6 text-2xl font-bold\">Welcome to Warp Dialog</h2>\n          <div className=\"flex justify-center gap-4\">\n            <Button variant=\"default\">Get Started</Button>\n            <Button variant=\"outline\">Learn More</Button>\n          </div>\n        </WarpDialogContent>\n      </WarpDialog>\n    </div>\n  )\n}\n",
      "type": "registry:ui",
      "target": "components/molecule-ui/example/warp-dialog-demo.tsx"
    }
  ]
}